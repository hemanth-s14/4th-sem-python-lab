    "def fn(n):\n",
    "    if n == 1:\n",
    "        return 0\n",
    "    elif n == 2:\n",
    "        return 1\n",
    "    else:\n",
    "        return fn(n-1) + fn(n-2)\n",
    "\n",
    "\n",
    "num = int(input(\"Enter a number : \"))\n",
    "if num > 0:\n",
    "    print(\"fn(\", num, \") = \", fn(num), sep=\"\")\n",
    "else:\n",
    "    print(\"Error in input\")"
   ]
  }
